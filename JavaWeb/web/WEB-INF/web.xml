<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>pack.Aservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>payServlet</servlet-name>
        <servlet-class>pack.payServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>payServlet</servlet-name>
        <url-pattern>/pay</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>registServlet</servlet-name>
        <servlet-class>pack.registServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registServlet</servlet-name>
        <url-pattern>/regist</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>tsfcount</servlet-name>
        <servlet-class>pack.tsfServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tsfcount</servlet-name>
        <url-pattern>/tsfcount</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>getreset</servlet-name>
        <servlet-class>pack.resetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getreset</servlet-name>
        <url-pattern>/reset</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>delete</servlet-name>
        <servlet-class>pack.DeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>change</servlet-name>
        <servlet-class>pack.ChangeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>change</servlet-name>
        <url-pattern>/change</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>request</servlet-name>
        <servlet-class>pack.RequsetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>request</servlet-name>
        <url-pattern>/request</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>verify</servlet-name>
        <servlet-class>pack.VImageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>verify</servlet-name>
        <url-pattern>/verify</url-pattern>
    </servlet-mapping>
</web-app>